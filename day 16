# dictionary --->



d={
    'name':'Rohit',
    'rollNo':12,
    'id':1,
    'age':13,
    'colour':'red'
}

print(d['name'])
print(d['age'])

colour='red'
d['color']='Blue'

print(d)

del d['name']

print(d)

for i in d:
    print(i,d[i])


car={
    'model':123,
    'color':'White',
    'ac':True
}

print(car)
for i in car:
    print(i,car[i])

car['wheels']=4
print(car)

del car['wheels']
print(car)

car['color']='grey'
print(car)









