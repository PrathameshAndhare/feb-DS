def sub(y,z):
    return y-z
    
    
qq=sub(32,12)

print(qq*2,qq)
 
 
 
def DICT(z):
    for (key,value) in z.items():
        print('Key is ',key," and value is ",value)
   
d={
    'Blue':1,
    'Orange':5,
    'Red':3,
}
a={
    'milk':1,
    'bread':2,
    'eggs':3
}
DICT(d)
DICT(a)


def fun2(lst):
    for i in range(len(lst)):
        lst[i]=lst[i].upper()


ss=['apple','mango','banana']
print(ss)
fun2(ss)

print(ss)


