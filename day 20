a=[1,2,3,4,5,6,-1,4,3]

max=a[0]
for i in range(len(a)):
    if(max<a[i]):
        max=a[i]
        
print(max)


b=[11,334,1,23,111]

max=b[0]
for i in range(len(b)):
    if(max<b[i]):
        max=b[i]
        
print(max)





def fun(a):
    max=a[0] #76
    for i in range(len(a)):
        if(max>a[i]):
            max=a[i]
        
    print(max)
        
        
fun([1,45,32,76,5,4,44,0])
fun([3,4,6,5,-1,34,56,123,32])

w=[2,3,4,5,1,2,33,13,21]

fun(w)


def SUM(a):
    sum=0# 1
    for i in range(len(a)):
        sum=sum+a[i]
    return sum
   
'''
SUM([1,2,3,4,5])
'''
qq=SUM([22,43,54,32,1,12,0])
print(qq)



