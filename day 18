a=['apple','banana','mango','orange']
b=2
print(dict.fromkeys(a,b))


print(d)


for i in d.keys():
    print(i)

for i in d.values():
    print(i)
    
    
for k,v in d.items():
    print(k,v) 
    
    
    
    
    
    
    
    
    

