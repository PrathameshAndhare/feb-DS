# string methods --->

#1. format ()


name='Rohit'
age=19
area='Pune'
print(name,age,area)


print('My name is :',name," and I am ",age,"years old",'I live in ',area)
print('My name is {0} and I am {1} years old and I live in {2}.'.format(name,age,area))

print('My name is {0} and I am {1} years old and I live in {2}.'.format('soham',20,'goa'))



print('My name is {0} and I am {1} years old and I live in {2}.'.format(area,name,age))
