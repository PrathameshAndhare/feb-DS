#  0123456
s='rainbow'
'''
w 6 ,-1 
o 5 ,-2
b 4, -3
n 3 -4
i 2 -5
a 1 -6
r 0 -7
'''
print('------------')
print(s[0],s[-7])
print(s[4],s[-3])
print('------------')

print(s[6])

print(s[-1])
print('------------')
print(s[7])
print(s[9])

# last index = length -1
# type(), len()


print(len('word'))

