# get 

d={
    'name':'Om',
    'age':23,
    'skills':['python','Js','c++','data structures']
}
d[12]='value'
print(d)
print(d.get('name'))

print(d.get('color'))

# keys()


print(d.keys())
# values()
print(d.values())

# items()
print(d.items())

# update({'key':'value'})
print(d)
d.update({'name':'Harsh'})
print(d)

d.update({'color':'red'})
print(d)

















