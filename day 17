# way 2 -->
# constructor-->
name='OM'
dd=dict(a=1,b=2,c=3,name=name)
print(dd)
dd['d']=4
print(dd)
del dd['a']
print(dd)



# way 3 ---> through list 
# dict(zip([keys],[values]]))
qq=dict(zip(['a','b','c'],[1,2,3]))
qq['c']=4
print(qq)

# way 4 ---> through tuple

z=dict([('a',1),('b',2),('c',3)])
print(z)


