print(a)


# insert()
#['Blue', 'orange', 'grey', 'green', 'red', 'pink', 'yellow', 'white', 'orange']
a.insert(2,'grey')
a.insert(-5,'green')
print(a)

# 3 extend 

y=[1,2,3]
z=[5,6,7,8]

y.extend(z)
print(y,z)
a=[1,2,3]
b=[0,5,4]
b.extend(a)
print(a,b)

